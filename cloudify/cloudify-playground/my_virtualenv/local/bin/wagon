#!/home/ubuntu/cloudify/my_virtualenv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'wagon==0.3.2','console_scripts','wagon'
__requires__ = 'wagon==0.3.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('wagon==0.3.2', 'console_scripts', 'wagon')()
    )
